{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "env": {
    "GROQ_API_KEY": "@GROQ_API_KEY",
    "QDRANT_URL": "@QDRANT_URL",
    "QDRANT_API_KEY": "@QDRANT_API_KEY",
    "DATABASE_URL": "@DATABASE_URL",
    "FRONTEND_URL": "@FRONTEND_URL"
  },
  "functions": {
    "api/index.py": {
      "runtime": "python3.12",
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}
